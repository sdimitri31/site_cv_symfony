{% extends 'base.html.twig' %}

{% block title %}Visit{% endblock %}

{% block body %}
    <h1>Visit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visit.id }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ visit.ip }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ visit.url }}</td>
            </tr>
            <tr>
                <th>Method</th>
                <td>{{ visit.method }}</td>
            </tr>
            <tr>
                <th>VisitedAt</th>
                <td>{{ visit.visitedAt ? visit.visitedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>VisitedBy</th>
                <td>{{ visit.visitedBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_visit_index') }}">back to list</a>

    <a href="{{ path('app_visit_edit', {'id': visit.id}) }}">edit</a>

    {{ include('visit/_delete_form.html.twig') }}
{% endblock %}
